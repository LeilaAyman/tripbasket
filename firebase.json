{
  "hosting": {
    "site": "tripbasket-sctkxj",
    "public": "build/web",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "compressionEnabled": true,
    "http2": "push",
    "http2PushLinks": [
      {
        "source": "/",
        "headers": [
          {
            "key": "Link",
            "value": "</flutter.js>; rel=preload; as=script, </main.dart.js>; rel=preload; as=script"
          }
        ]
      }
    ],
    "headers": [
      {
        "source": "/index.html",
        "headers": [
          { "key": "Cache-Control", "value": "no-cache, no-store, must-revalidate" },
          { "key": "Pragma", "value": "no-cache" },
          { "key": "Expires", "value": "0" },
          { "key": "X-Content-Type-Options", "value": "nosniff" },
          { "key": "X-Frame-Options", "value": "DENY" },
          { "key": "X-XSS-Protection", "value": "1; mode=block" },
          { "key": "Referrer-Policy", "value": "strict-origin-when-cross-origin" },
          { "key": "Permissions-Policy", "value": "geolocation=(), microphone=(), camera=()" }
        ]
      },
      {
        "source": "/manifest.json",
        "headers": [
          { "key": "Cache-Control", "value": "public, max-age=86400" },
          { "key": "Content-Type", "value": "application/manifest+json" }
        ]
      },
      {
        "source": "**/main.dart.js",
        "headers": [
          { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" },
          { "key": "Content-Type", "value": "application/javascript" },
          { "key": "Vary", "value": "Accept-Encoding" }
        ]
      },
      {
        "source": "**/main.dart.js.gz",
        "headers": [
          { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" },
          { "key": "Content-Encoding", "value": "gzip" },
          { "key": "Content-Type", "value": "application/javascript" },
          { "key": "Vary", "value": "Accept-Encoding" }
        ]
      },
      {
        "source": "**/main.dart.js.br",
        "headers": [
          { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" },
          { "key": "Content-Encoding", "value": "br" },
          { "key": "Content-Type", "value": "application/javascript" },
          { "key": "Vary", "value": "Accept-Encoding" }
        ]
      },
      {
        "source": "**/*.js",
        "headers": [
          { "key": "Cache-Control", "value": "public,max-age=31536000,immutable" },
          { "key": "Vary", "value": "Accept-Encoding" }
        ]
      },
      {
        "source": "**/*.css",
        "headers": [
          { "key": "Cache-Control", "value": "public,max-age=31536000,immutable" },
          { "key": "Vary", "value": "Accept-Encoding" }
        ]
      },
      {
        "source": "**/*.@(woff2|webp)",
        "headers": [
          { "key": "Cache-Control", "value": "public,max-age=31536000,immutable" },
          { "key": "Vary", "value": "Accept-Encoding" }
        ]
      },
      {
        "source": "**/*.@(js.gz|css.gz)",
        "headers": [
          { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" },
          { "key": "Content-Encoding", "value": "gzip" },
          { "key": "Vary", "value": "Accept-Encoding" }
        ]
      },
      {
        "source": "**/*.js.br",
        "headers": [
          { "key": "Cache-Control", "value": "public,max-age=31536000,immutable" },
          { "key": "Content-Encoding", "value": "br" },
          { "key": "Content-Type", "value": "application/javascript" },
          { "key": "Vary", "value": "Accept-Encoding" }
        ]
      },
      {
        "source": "**/*.css.br",
        "headers": [
          { "key": "Cache-Control", "value": "public,max-age=31536000,immutable" },
          { "key": "Content-Encoding", "value": "br" },
          { "key": "Content-Type", "value": "text/css" },
          { "key": "Vary", "value": "Accept-Encoding" }
        ]
      },
      {
        "source": "**/*.@(woff2.br|webp.br|json.br|html.br)",
        "headers": [
          { "key": "Cache-Control", "value": "public,max-age=31536000,immutable" },
          { "key": "Content-Encoding", "value": "br" },
          { "key": "Vary", "value": "Accept-Encoding" }
        ]
      },
      {
        "source": "**/*.webp.br",
        "headers": [
          { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" },
          { "key": "Content-Encoding", "value": "br" },
          { "key": "Content-Type", "value": "image/webp" },
          { "key": "Vary", "value": "Accept" }
        ]
      },
      {
        "source": "/assets/images/optimized/**/*.webp",
        "headers": [
          { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" },
          { "key": "Content-Type", "value": "image/webp" },
          { "key": "Vary", "value": "Accept" }
        ]
      },
      {
        "source": "/assets/images/optimized/**/*.webp.gz",
        "headers": [
          { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" },
          { "key": "Content-Type", "value": "image/webp" },
          { "key": "Content-Encoding", "value": "gzip" },
          { "key": "Vary", "value": "Accept" }
        ]
      },
      {
        "source": "/assets/**",
        "headers": [
          { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" },
          { "key": "Cross-Origin-Resource-Policy", "value": "cross-origin" }
        ]
      },
      {
        "source": "**/*.@(png|jpg|jpeg|gif|svg|webp|ico)",
        "headers": [
          { "key": "Cache-Control", "value": "public, max-age=2592000" },
          { "key": "Vary", "value": "Accept" }
        ]
      },
      {
        "source": "**/fonts/**",
        "headers": [
          { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" },
          { "key": "Cross-Origin-Resource-Policy", "value": "cross-origin" }
        ]
      },
      {
        "source": "**/*.@(woff|woff2|ttf|eot)",
        "headers": [
          { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" },
          { "key": "Cross-Origin-Resource-Policy", "value": "cross-origin" }
        ]
      },
      {
        "source": "/robots.txt",
        "headers": [
          { "key": "Cache-Control", "value": "public, max-age=86400" },
          { "key": "Content-Type", "value": "text/plain" }
        ]
      },
      {
        "source": "/sitemap*.xml",
        "headers": [
          { "key": "Cache-Control", "value": "public, max-age=86400" },
          { "key": "Content-Type", "value": "application/xml" }
        ]
      },
      {
        "source": "/sw.js",
        "headers": [
          { "key": "Cache-Control", "value": "no-cache, no-store, must-revalidate" },
          { "key": "Content-Type", "value": "application/javascript" }
        ]
      }
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "cleanUrls": true,
    "trailingSlash": false
  },
  "functions": {
    "source": "functions"
  },
  "firestore": {
    "rules": "firestore.rules"
  },
  "storage": {
    "rules": "storage.rules"
  },
  "emulators": {
    "storage": {
      "port": 9199
    },
    "ui": {
      "enabled": true,
      "port": 4000
    }
  }
}
